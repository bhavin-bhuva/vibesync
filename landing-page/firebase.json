{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "deploy-to-firebase.sh",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000
    }
  }
}